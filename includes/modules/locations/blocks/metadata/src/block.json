{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "sim/locationmeta",
	"version": "0.1.0",
	"title": "Location meta data",
	"category": "widgets",
	"description": "All location details",
	"textdomain": "sim",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"lock": {
			"type": "object",
			"default": {
				"move": true,
				"remove": true
			}
		},
		"location":{
			"type": "string",
			"default": ""
		}
	}
}
